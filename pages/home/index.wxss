/* pages/home/index.wxss */
.home-swiper .swiper-list {
  width: 750rpx;
  height: 340rpx;
}
.home-swiper .swiper-list image {
  width: 100%;
}
.nav-bar {
  padding: 20rpx;
  display: flex;
}
.nav-bar .nav-bar-top {
  flex: 1;
}
.nav-bar .nav-bar-top image {
  width: 100%;
}
.floor .floor-title {
  padding: 10rpx 0;
}
.floor .floor-title image {
  width: 100%;
}
.floor .floor-list {
  overflow: hidden;
}
.floor .floor-list .list {
  float: left;
  width: 33.33%;
}
.floor .floor-list .list image {
  width: 100%;
  height: 100%;
}
.floor .floor-list .list:nth-last-child(-n + 4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor .floor-list .list:nth-child(2),
.floor .floor-list .list:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
